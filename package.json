{
  "name": "@vafast/helmet",
  "version": "0.0.4",
  "description": "Plugin for Vafast for security headers",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsdown",
    "dev": "npx tsx watch example/index.ts",
    "test": "npx vitest run",
    "release": "npm run build && npm run test && npx bumpp && npm publish --access=public"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "vafast",
    "helmet",
    "security"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vafast/vafast-helmet"
  },
  "author": {
    "name": "saltyAom",
    "url": "https://github.com/SaltyAom",
    "email": "saltyaom@gmail.com"
  },
  "homepage": "https://github.com/vafast/vafast-helmet",
  "bugs": "https://github.com/vafast/vafast-helmet/issues",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "peerDependencies": {
    "vafast": ">=0.3.11"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "rimraf": "^6.0.1",
    "tsdown": "^0.19.0-beta.4",
    "typescript": "^5.4.5",
    "vafast": ">=0.3.11",
    "vitest": "^4.0.16"
  }
}
